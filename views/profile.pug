extends layout

block content
  h2 My Profile
  br
  div
    p Hello, #{user.displayName}
  br
  table
    thead
      tr
        th Claim
        th Value
    tbody
      for value, claim in user
        tr
          td #{claim}
          td #{value}
  form(method="post", action="/authenticate")
    button(type="submit") Perform Action here
